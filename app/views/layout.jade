  body
    #subscribeModal.modal.fade(role=dialog)
     .modal-dialog
       .modal-content
         .modal-header
            button.close(type='button',data-dismiss='modal')
              | &times;
            h4.modal-title Subscribe to SOFTSKY Blog                       
         .modal-body             
            form(role='form')
             .form-group
              label(for='subscribe_firstName') First Name
              input#subscribe_firstName.form-control.btn-space(type='text', placeholder='John')
             .form-group
              label(for='subscribe_lastName') Last Name
              input#subscribe_lastName.form-control.btn-space(type='text', placeholder='Doe')
             .form-group
              label(for='subscribe_email') Email
              input#subscribe_email.form-control.btn-space(type='text', placeholder='john.doe@gmail.com')
         .modal-footer
            h6 Thank you for your interest
            button.btn.btn-space.btn-primary Subscribe
            button.btn.btn-space.btn-default(data-dismiss='modal') Dismiss
            
    #signUp_signUpModal.modal.fade(role=dialog)
     .modal-dialog
       .modal-content
         .modal-header
            button.close(type='button',data-dismiss='modal')
              | &times;
            h4.modal-title Sign Up                       
         .modal-body             
            form(role='form')
             .form-group
              label(for='signUp_firstName') First Name
              input#signUp_firstName.form-control.btn-space(type='text', placeholder='John')
             .form-group
              label(for='signUp_lastName') Last Name
              input#signUp_lastName.form-control.btn-space(type='text', placeholder='Doe')
             .form-group
              label(for='signUp_email') Email
              input#signUp_email.form-control.btn-space(type='text', placeholder='john.doe@example.com')
             .form-group
              label.sr-only(for='signUp_website') Email
              .input-group
                .input-group-addon http://
                input#signUp_website.form-control.btn-space(type='text', placeholder='example.com')
         .modal-footer
            h6 Thank you for your registration
            button.btn.btn-space.btn-primary Sign up
            button.btn.btn-space.btn-default(data-dismiss='modal') Dismiss

    #signIn_signInModal.modal.fade(role=dialog)
     .modal-dialog
       .modal-content
         .modal-header
            button.close(type='button',data-dismiss='modal')
              | &times;
            h4.modal-title Sign In
         .modal-body             
            form(role='form')
             .form-group
              label(for='signIn_firstName') Email
              input#signIn_firstName.form-control.btn-space(type='text', placeholder='john.doe@example.com')
             .form-group
              label(for='signIn_lastName') Password
              input#signIn_lastName.form-control.btn-space(type='password', placeholder='')
         .modal-footer
            button.btn.btn-space.btn-primary Sign In
            button.btn.btn-space.btn-default(data-dismiss='modal') Dismiss
            
   .floating-side-button
      button.btn.btn-info.btn-lg(type='button',data-toggle='modal',data-target='#subscribeModal') Subscribe
