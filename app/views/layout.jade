          form.navbar-form.navbar-right(role='form')
            .form-group
              button.btn.btn-space.btn-success(type='button',data-toggle='modal',data-target='#signInModal') Sign in
              button.btn.btn-space.btn-success.btn-info(type='button',data-toggle='modal',data-target='#signUpModal') Sign up
            .form-group
              a(href='http://www.facebook.com/softsky')
